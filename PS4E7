    "        1:0.12,\n",
    "        0:0.88\n",
    "    }\n",
    ")\n",
    "RF.fit(x_train,y_train)\n",
    "y_pred_RF = DT.predict(x_test)\n",
    "print(\"Accuracy:\", accuracy_score(y_test, y_pred_RF))"
   ]

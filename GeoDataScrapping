    "def get_all_links(soup):  \n",
    "    listlink = []\n",
    "    links = soup.find_all('a')\n",
    "    for link in links:\n",
    "        part = link['href']\n",
    "        listlink.append(f'https://villageinfo.in{part}')\n",
    "    return listlink"
   ]
  },
